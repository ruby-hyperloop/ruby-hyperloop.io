<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <meta name="google-site-verification" content="qjqVOvRhHfsuDbLOvaAnj2mkUZJ9Xwzg84TMVXduvUc" />

    <meta charset="UTF-8">
    <title>Hyperloop<%= ' - ' + current_page.data.title unless current_page.data.title.nil? %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <script src="https://unpkg.com/react@15/dist/react.min.js"></script>
    <script src="https://unpkg.com/react-dom@15/dist/react-dom.min.js"></script>

    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/marked/0.3.5/marked.min.js"></script>


    <!-- Opal and Hyperloop -->
    <script src="https://rawgit.com/ruby-hyperloop/hyperloop-js/master/opal-compiler.min.js"></script>
    <script src="https://rawgit.com/ruby-hyperloop/hyperloop-js/master/hyperloop.min.js"></script>

    <!-- If you want local copies... -->
    <!-- <%= javascript_include_tag 'opal-compiler.js' %> -->
    <!-- <%= javascript_include_tag 'hyperloop.js' %> -->

    <script src="https://npmcdn.com/tether@1.2.4/dist/js/tether.min.js"></script>

    <%= javascript_include_tag 'bootstrap.min.js' %>
    <%= javascript_include_tag 'codemirror.js' %>
    <%= javascript_include_tag 'ruby.js' %>
    <%= javascript_include_tag 'matchbrackets.js' %>
    <%= javascript_include_tag 'react_player.js' %>
    <%= javascript_include_tag 'highlight.pack.js' %>


    <!-- Components are compiled by Hyperloop Express -->
    <%= partial("/components/_code_mirror", :locals => { :hyperlooplogosource => '/images/hyperloop-logo-small-white.png'}) %>
    <%= partial("/components/_live_edit") %>
    
    <%= partial("/components/_toggle_codemirror", :locals => { :hyperlooplogosource => '/images/hyperloop-logo-small-white.png'}) %>
    <%= partial("/components/_toggle_code") %>

    <%= stylesheet_link_tag 'bootstrap.min.css' %>
    <%= stylesheet_link_tag 'typography.css' %>
    <%= stylesheet_link_tag 'override.css' %>
    <%= stylesheet_link_tag 'divtable.css' %>
    <!-- <%= stylesheet_link_tag 'code.css' %> -->
    <%= stylesheet_link_tag 'github.css' %>
    <%= stylesheet_link_tag 'highlighting.css' %>
    <%= stylesheet_link_tag 'codemirror.css' %>
    <%= stylesheet_link_tag 'monokai-sublime.css' %>

    <%= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed" %>

    <!-- Favicons -->

    <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/images/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/images/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="/images/manifest.json">
    <link rel="mask-icon" href="/images/safari-pinned-tab.svg" color="#e81176">
    <meta name="theme-color" content="#ffffff">

  </head>

  <body>
    <div class="navbarbackgroundcolor">
      <div class="container">
        <%= partial "navigation"%>
      </div>
    </div>

    <% if current_page.path.include?("tutorials")
        current_page.data.title = "Tutorials, screencasts & videos"
      end
    %>

    <div class="jumbotron page-header">
      <div class="container">

        <div class="row hidden-sm-down">
          <div class="col-md-2">
            <div class="hyperlooplogo">
            </div>
          </div>
          <div class="col-md-8">

            <h1 class="display-4 project-name">Hyperloop</h1>

            <h4 class="display-7 project-tagline">
              
              <%=
              current_page.data.title ? current_page.data.title : "The Complete Isomorphic Ruby Framework"
              %>
            </h4>
          </div>
          
        </div>

        <div class="row hidden-md-up">
          <div class="col-md-3">
            <div class="hyperlooplogo">
            </div>
          </div>
          <div class="col-md-9">
            <h1 class="h1 project-name center-text">Hyperloop</h1>
            </br>
            <h2 class="h5 project-tagline center-text">
              <%=
              current_page.data.title ? current_page.data.title : "The Complete Isomorphic Ruby Framework"
              %>
            </h2>
          </div>
          
        </div>


      </div>
    </div>

    <div class="page-header-underline">
      <div class="container">

      </div>
    </div>

    <div class="container">
      <div class="row">
        <div class="col-md-3 sidenavcol">
          <%= partial "_sidenav" %>
          <br>
        </div>
        <div class="col-md-9 main-content">
          <%= yield %>
        </div>
      </div>
    </div>

    <div class="page-footer-upperline">
      <div class="container">

      </div>
    </div>

    <%= partial "footer"%>

    <script>
      (function(Opal) {
        var self = Opal.top, $scope = Opal, nil = Opal.nil, $breaker = Opal.breaker, $slice = Opal.slice;
        Opal.add_stubs(['$puts']);
        return self.$puts("Opal ok")
      })(Opal);
    </script>

    <!-- <script   src="http://code.jquery.com/jquery-3.1.1.min.js"   integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="   crossorigin="anonymous"></script> -->


    <script type="text/ruby">puts "Hyperloop JS ok"</script>

    


  </body>
</html>
